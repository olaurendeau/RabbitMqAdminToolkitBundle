{
    "name": "olaurendeau/rabbit-mq-admin-toolkit-bundle",
    "description": "Automate rabbitmq vhost's configuration creation / update",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.4",
        "symfony/http-client": "^4.4|^5.0",
        "richardfullmer/rabbitmq-management-api": "^2.0",
        "symfony/config": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Laurendeau",
            "email": "o.laurendeau@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Ola\\RabbitMqAdminToolkitBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
